{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommanded", "airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "ecmaFeatures": {
    "jsx": true
  }
}
